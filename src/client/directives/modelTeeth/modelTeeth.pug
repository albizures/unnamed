.model-teeth.big
  div
    md-switch(ng-model="isBig",ng-change='setStatus(isBig)', aria-label="Modelo") {{ isBig? 'Grande' : 'Peque√±a'  }}
    .content-model
      img(ng-src='{{url}}')
      .content-areas
        button.area(
          ng-repeat='area in areas',
          ng-style='area',
          ng-attr-id='{{area.key}}'
          ng-click='handler(area)',
          type='button'
        )
          md-tooltip
            | {{(getElements(area.key).length || 0)}} elementos